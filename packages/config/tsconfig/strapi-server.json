{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Strapi Server",
  "extends": "./base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2020"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noImplicitThis": true,
    "target": "ES2019"
  }
}
