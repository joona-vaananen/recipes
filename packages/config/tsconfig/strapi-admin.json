{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Strapi Admin",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "ESNext"
  }
}
